                            	     1: ; ***********************************************************************************************************
                            	     2: ;
                            	     3: ; 										Simple assembly program.
                            	     4: ;
                            	     5: ;		Originally written by pweingar
                            	     6: ;		Extended by Paul Robson
                            	     7: ;
                            	     8: ; ***********************************************************************************************************
                            	     9: ;
                            	    10: ;	To run me, make asample or f68.exe asample\sample.a68 and run with F5, press F1 when the command line appears
                            	    11: ;
                            	    12: 
                            	    13: 				org 	$10000
                            	    14: 
                            	    15: start:          									; start tells us where to run from, because of -exec=start in the command line.
                            	    16: 				;
                            	    17: 				; 		Write Hello world using the O/S
                            	    18: 				;
00:00010000 7013            	    19: 				move.l #$13,d0                      ; sys_chan_write command #
00:00010002 7200            	    20:                 clr.l d1                            ; Channel #0 (the display)
00:00010004 243C0001005E    	    21:                 move.l #greet,d2                    ; Pointer to message
00:0001000A 760E            	    22:                 move.l #greet_end-greet+1,d3        ; Length of message
00:0001000C 4E4F            	    23:                 trap #15 							; handles system functions
                            	    24:                 ;
                            	    25:                 ;		Write Hello world by writing straight to text memory
                            	    26:                 ;		do it 32 times from the screen top.
                            	    27:                 ;
00:0001000E 41F9FEC60000    	    28:                 lea.l 	$FEC60000,a0 				; screen text memory here
00:00010014 103C0020        	    29:                 move.b 	#32,d0 						; do it 32 times.
00:00010018 43FA0044        	    30: repeatmessage:  lea.l 	greet,a1 					; message here
                            	    31: copymessage:
00:0001001C 10D9            	    32: 				move.b 	(a1)+,(a0)+ 				; copy byte over.
00:0001001E 4A51            	    33: 				tst 	(a1) 						; is it zero.
00:00010020 66FA            	    34: 				bne 	copymessage 				; do until trailing 0 found.
00:00010022 56C8FFF4        	    35: 				dbne 	d0,repeatmessage 			; do it again !
                            	    36: 
                            	    37: track:
00:00010026 2039FEC00208    	    38: 				move.l 	($FEC00208),d0
00:0001002C 23C0FEC60000    	    39: 				move.l 	d0,($FEC60000)	
00:00010032 2039FEC00210    	    40: 				move.l 	($FEC00210),d0
00:00010038 23C0FEC60006    	    41: 				move.l 	d0,($FEC60006)	
00:0001003E 2039FEC00218    	    42: 				move.l 	($FEC00218),d0
00:00010044 23C0FEC6000C    	    43: 				move.l 	d0,($FEC6000C)	
00:0001004A 2039FEC00220    	    44: 				move.l 	($FEC00220),d0
00:00010050 23C0FEC60012    	    45: 				move.l 	d0,($FEC60012)	
00:00010056 60CE            	    46: 				bra 	track
                            	    47: 				;
                            	    48: 				; 		Exit the program.
                            	    49: 				;
00:00010058 7000            	    50: done:           clr.l d0                            ; sys_exit
00:0001005A 7200            	    51:                 clr.l d1                            ; Return value = 0
00:0001005C 4E4F            	    52:                 trap #15
                            	    53: 
